{
  "name": "shelf-app-xxx",
  "version": "0.0.0",
  "description": "Awesome app description",
  "homepage": "https://github.com/shelfio/shelf-app-xxx",
  "repository": {
    "type": "git",
    "url": "https://github.com/shelfio/shelf-app-xxx"
  },
  "license": "UNLICENSED",
  "author": "Yaroslav Chapelskyi",
  "scripts": {
    "build": "next build",
    "coverage": "yarn test --coverage",
    "dev": "next",
    "export": "next export",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.json --fix",
    "lint:ci": "eslint . --ext .js,.jsx,.ts,.tsx,.json --quiet",
    "lint:styles": "stylelint '**/*.{scss,css,less,sass}' --fix",
    "start": "next start",
    "test": "TZ=UTC jest",
    "test-update": "TZ=UTC jest --updateSnapshot",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{html,md,yml,graphql,scss,css,less,sass}": [
      "prettier --write",
      "git add"
    ],
    "*.{scss,css,less,sass}": [
      "stylelint --fix",
      "git add"
    ],
    "*.{ts,js,json,tsx,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": "@shelf/prettier-config",
  "dependencies": {
    "@datadog/browser-logs": "1.25.2",
    "@shelf/bem": "1.3.0",
    "@shelf/client-helpers": "1.4.0",
    "@shelf/i18n": "7.2.82",
    "@shelf/icons": "0.8.0",
    "@shelf/react-basic-components": "5.10.2",
    "@shelf/sdk": "5.6.44",
    "@zeit/next-sass": "1.0.1",
    "i18next": "19.8.3",
    "lodash": "4.17.21",
    "next": "10.0.1",
    "next-redux-wrapper": "6.0.2",
    "node-sass": "4.14.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "11.7.3",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@shelf/eslint-config": "0.24.0",
    "@shelf/prettier-config": "0.0.7",
    "@shelf/test-utils": "0.1.238",
    "@shelf/tsconfig": "0.0.5",
    "@shelf/types": "0.71.21",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.1",
    "@types/jest": "26.0.22",
    "@types/lodash": "4.14.168",
    "@types/react": "16.14.5",
    "@types/react-dom": "16.9.12",
    "@types/react-redux": "7.1.16",
    "@types/styled-components": "5.1.9",
    "@types/testing-library__jest-dom": "5.9.5",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-dynamic-import-webpack": "1.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.24.0",
    "eslint-plugin-jest-dom": "3.8.0",
    "eslint-plugin-testing-library": "3.10.2",
    "husky": "4.3.8",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-next-dynamic": "1.0.1",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.5.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "stylelint": "13.12.0",
    "stylelint-config-recommended": "4.0.0",
    "typescript": "4.0.5"
  }
}
