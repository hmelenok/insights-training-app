{
  "name": "insights-training-app",
  "version": "0.0.0",
  "description": "Awesome app description",
  "homepage": "https://github.com/shelfio/insights-training-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/shelfio/insights-training-app"
  },
  "license": "UNLICENSED",
  "author": "Shelf",
  "scripts": {
    "build": "next build",
    "coverage": "yarn test --coverage",
    "dev": "next",
    "export": "next export",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.json --fix",
    "lint:ci": "eslint . --ext .js,.jsx,.ts,.tsx,.json --quiet",
    "lint:styles": "stylelint '**/*.{scss,css,less,sass}' --fix",
    "start": "next start",
    "test": "TZ=UTC jest",
    "test-update": "TZ=UTC jest --updateSnapshot",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{html,md,yml,graphql,scss,css,less,sass}": [
      "prettier --write"
    ],
    "*.{scss,css,less,sass}": [
      "stylelint --fix"
    ],
    "*.{ts,js,json,tsx,jsx}": [
      "eslint --fix"
    ]
  },
  "prettier": "@shelf/prettier-config",
  "dependencies": {
    "@datadog/browser-logs": "2.8.1",
    "@zeit/next-sass": "1.0.1",
    "debug": "4.3.1",
    "fetch-ponyfill": "7.1.0",
    "graphql-request": "3.4.0",
    "i18next": "20.2.2",
    "lodash": "4.17.21",
    "next": "10.0.3",
    "next-redux-wrapper": "6.0.2",
    "node-sass": "5.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.8.15",
    "react-redux": "7.2.4",
    "redux": "4.1.0",
    "redux-devtools-extension": "2.13.9",
    "redux-thunk": "2.3.0",
    "styled-components": "5.3.0",
    "swr": "0.5.6"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@shelf/babel-config": "0.1.2",
    "@shelf/eslint-config": "1.1.0",
    "@shelf/prettier-config": "1.0.0",
    "@shelf/tsconfig": "0.0.6",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.6",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.168",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "@types/react-redux": "7.1.16",
    "@types/styled-components": "5.1.9",
    "@types/testing-library__jest-dom": "5.9.5",
    "eslint": "7.25.0",
    "eslint-plugin-jest-dom": "3.8.1",
    "eslint-plugin-testing-library": "4.2.0",
    "husky": "4.3.8",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-next-dynamic": "1.0.1",
    "jest-styled-components": "7.0.4",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "typescript": "4.2.4"
  }
}
